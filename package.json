{
  "private": true,
  "name": "todomvc-vercel",
  "version": "0.0.0-development",
  "description": "Example app deployed to Vercel and testing using Cypress/included Docker image",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "cy:open": "cypress open",
    "dev": "start-test 3000 cy:open",
    "lint": "eslint 'src/**/*.js' 'cypress/**/*.js'"
  },
  "dependencies": {
    "react": "16",
    "react-dom": "16",
    "react-scripts": "3.2"
  },
  "devDependencies": {
    "cypress": "12.0.2",
    "cypress-grep": "2.14.0",
    "eslint-plugin-cypress": "2.12.1",
    "prettier": "2.2.1",
    "start-server-and-test": "1.14.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:cypress/recommended"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
